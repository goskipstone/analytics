  },
  "scripts": {
    "start": "npm run start-dev",
    "lint": "standard",
    "dev": "better-npm-run start-dev",
    "build-dev": "better-npm-run build-dev",
    "deploy-dev": "npm run build-dev && aws s3 sync ./dist s3://skipstone-analytics --exclude '.DS_Store' --profile skipstone --cache-control 'max-age=604800' --expires 2100-01-01T00:00:00Z"
  },
  "betterScripts": {
    "start-dev": {
      "command": "webpack-dev-server --progress --hot --colors --history-api-fallback --display-error-details",
      "env": {
        "DEVELOPMENT": true,
        "DEVTOOLS": true,
        "API_PATH": "https://qa-api.skipstone.marketing/api/",
        "REDUX_PREFIX": "skipstone/analytics"
      }
    },
    "build-dev": {
      "command": "webpack -p",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "issue",
        "master",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeadGrabr/webpack-static-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeadGrabr/webpack-static-boilerplate/issues"
  },
  "homepage": "https://github.com/LeadGrabr/webpack-static-boilerplate#readme",
  "standard": {
    "globals": [
      "$",
      "If",
      "Else",
      "Choose",
      "When",
      "Otherwise"
    ],
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "jsx-control-statements",
      "transform-decorators-legacy"
    ]
  }
}
